{
    "version": 2,
    "builds": [
      {
        "src": "debug_server.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "debug_server.py"
      }
    ],
    "env": {
      "FLASK_ENV": "production",
      "VERCEL": "1"
    },
    "functions": {
      "debug_server.py": {
        "maxDuration": 30
      }
    }
  }
