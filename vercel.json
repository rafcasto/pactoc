{
  "version": 2,
  "builds": [
    {
      "src": "backend/run.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/run.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task"
  },
  "functions": {
    "backend/run.py": {
      "includeFiles": "backend/**"
    }
  }
}
